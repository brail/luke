{
  "name": "@luke/api",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "tsc",
    "start": "node --require ./dist/instrument.js dist/server.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "seed": "prisma db push && ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.0",
    "@fastify/helmet": "^12.0.0",
    "@fastify/rate-limit": "^10.0.0",
    "@grpc/grpc-js": "^1.11.0",
    "@luke/core": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.52.0",
    "@opentelemetry/instrumentation-fastify": "^0.38.0",
    "@opentelemetry/instrumentation-http": "^0.52.0",
    "@opentelemetry/instrumentation-undici": "^0.3.0",
    "@opentelemetry/resources": "^1.25.0",
    "@opentelemetry/sdk-node": "^0.52.0",
    "@opentelemetry/semantic-conventions": "^1.25.0",
    "@prisma/client": "^6.0.0",
    "@prisma/instrumentation": "^5.18.0",
    "@trpc/server": "^11.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "argon2": "^0.41.1",
    "fastify": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "ldapjs": "^3.0.7",
    "next-auth": "5.0.0-beta.29",
    "nodemailer": "^6.9.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.2.2",
    "pino-std-serializers": "^7.0.0",
    "prisma": "^6.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/ldapjs": "^2.2.5",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^1.0.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
